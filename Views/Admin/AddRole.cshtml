@model Microsoft.AspNetCore.Identity.IdentityRole
<form asp-controller="Admin"
      asp-action="AddRole" method="post">
    <h2>Add a Role</h2>
    <label asp-for="Name"></label>
    <input asp-for="Name" />
    <br />
    <!--makes register button on registration form work-->
    <button type="submit">Add Role</button>
</form>
<p><a asp-controller="Admin" asp-action="AllRole">Back to All Role List</a></p>
<br />
<p><a asp-controller="Admin" asp-action="Index">Back to Administrator Homepage</a></p>